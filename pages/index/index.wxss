.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cate {
  display: flex;
}
.index_cate navigator {
  flex: 1;
  padding: 28rpx;
}
.index_cate image {
  width: 100%;
}
.index_floor .floor_group {
  overflow: hidden;
  padding: 20rpx 0;
}
.index_floor .floor_group .floor_title image {
  width: 100%;
}
.index_floor .floor_group .floor_list {
  float: left;
  width: 32.42%;
}
.index_floor .floor_group .floor_list:nth-last-child(-n+4) {
  height: 26.42711207vw;
  border-left: solid #fff 10rpx;
}
.index_floor .floor_group .floor_list:nth-last-child(2),
.index_floor .floor_group .floor_list:nth-last-child(3) {
  border-bottom: 10rpx solid #fff;
}
.index_floor .floor_group .floor_list image {
  width: 100%;
  height: 100%;
}
